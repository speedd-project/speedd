          },
          {
            "name": "Count",
            "aggregationType": "Count",
            "Transaction": "1"
          }
        ],
        "evaluationPolicy": "Deferred",
        "cardinalityPolicy": "Single",
        "internalSegmentation": [],
        "derivedEvents": [

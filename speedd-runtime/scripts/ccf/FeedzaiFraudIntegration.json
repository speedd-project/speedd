    "epas": [
    
      {
        "name": "CP_SuddenCardUseNearExpirationDateEPA",
        "createdDate": "Tue Aug 05 2014",
        "epaType": "Aggregate",
        "context": "CompositeInitiatedFirstTransaction20mins",
        "inputEvents": [
          {
            "name": "Transaction",
            "filterExpression": "Transaction.cvv_validation == 16 && Transaction.is_cnp == 0 && (Transaction.card_exp_date != null && DayDiff(Transaction.card_exp_date,Transaction.OccurrenceTime)<=1 && CompareDates(Transaction.OccurrenceTime,Transaction.card_exp_date) != 1)",
            "consumptionPolicy": "Consume",
            "instanceSelectionPolicy": "First"
          }
        ],
        "computedVariables": [
          {
            "name": "Count",
            "aggregationType": "Count",
            "Transaction": "1"
          }
        ],
        "assertion": "Count >= 3",
        "evaluationPolicy": "Immediate",
        "cardinalityPolicy": "Unrestricted",
        "internalSegmentation": [],
        "derivedEvents": [
          {
            "name": "SuddenCardUseNearExpirationDate",
            "reportParticipants": false,
            "expressions": {
              "Certainty": "Sigmoid(-1.674, 0.693,Count)",
              "Duration": "0",
              "card_pan": "context.CardNumberSegmentation",
              "TransactionsCount": "Count",
              "is_cnp": "0",
              "transaction_ids": "ArrayConvert(Transaction.transaction_id)",
              "timestamps":"ArrayConvert(Transaction.DetectionTime)",
              "acquirer_country":"ArrayConvert(Transaction.acquirer_country)",
              "card_country":"ArrayGet(Transaction.card_country,ArraySize(Transaction.card_country)-1)"
            }
          }
        ]
      },
      {
        "name": "CP_TransactionsInFarAwayPlacesEPA",
        "createdDate": "Wed Aug 06 2014",
        "epaType": "Sequence",
        "context": "CompositeInitiatedFirstTransaction15mins",
        "inputEvents": [
          {
            "name": "Transaction",
            "alias": "T1",
            "filterExpression": "T1.is_cnp == 0",
            "consumptionPolicy": "Consume",
            "instanceSelectionPolicy": "First"
          },
          {
            "name": "Transaction",
            "alias": "T2",
            "filterExpression": "T2.is_cnp == 0",
            "consumptionPolicy": "Consume",
            "instanceSelectionPolicy": "Override"
          }
        ],
        "computedVariables": [],
        "assertion": "T1.acquirer_country != T2.acquirer_country",
        "evaluationPolicy": "Immediate",
        "cardinalityPolicy": "Unrestricted",
        "internalSegmentation": [],
        "derivedEvents": [
          {
            "name": "TransactionsInFarAwayPlaces",
            "reportParticipants": false,
            "expressions": {
              "Certainty": "1.0",
              "Duration": "0",
              "card_pan": "context.CardNumberSegmentation",
              "card_country":"T1.card_country",
              "acquirer_country":"ArrayConvert(Split(Concat(Concat(T1.acquirer_country,\",\"),T2.acquirer_country),\",\"))",
              "timestamps":"ArrayConvert(Split(Concat(Concat(T1.DetectionTime,\",\"),T2.DetectionTime),\",\"))",
              "transaction_ids":"ArrayConvert(Split(Concat(Concat(T1.transaction_id,\",\"),T2.transaction_id),\",\"))"
            }
          }
        ]
      }
      ,
      {
        "name": "AggregateStatsEPA",
